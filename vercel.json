{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "data/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "^/$",
            "dest": "/api/index.js"
        },
        {
            "src": "^/home$",
            "dest": "/api/home.js"
        },
        {
            "src": "^/api/(.*)$",
            "dest": "/api/$1.js"
        }
    ]
}